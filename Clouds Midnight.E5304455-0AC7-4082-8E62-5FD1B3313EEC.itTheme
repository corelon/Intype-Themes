			title: "HTML Attribute"
			scope: "entity.other.attribute-name.html"
			settings: {
				foreground: "#A46763"
			}
		}
		{
			title: "Keyword -> Operator"
			scope: "keyword.operator"
			settings: {
				foreground: "#4B4B4B"
			}
		}
		{
			title: "Storage"
			scope: "storage"
			settings: {
				font_style: ""
				foreground: "#E92E2E"
			}
