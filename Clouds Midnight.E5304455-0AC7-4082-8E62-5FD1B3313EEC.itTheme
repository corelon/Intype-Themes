{
	title: "Clouds Midnight"
	settings: [
		{
			settings: {
				background: "#191919"
				caret: "#7DA5DC"
				foreground: "#929292"
				invisibles: "#BFBFBF"
				line_highlight: "#D7D7D708"
				selection: "#000000"
			}
		}
		{
			title: "Comment"
			scope: "comment"
			settings: {
				font_style: ""
				foreground: "#3C403B"
			}
		}
		{
			title: "String"
			scope: "string"
			settings: {
				font_style: ""
				foreground: "#5D90CD"
			}
		}
		{
			title: "Number"
			scope: "constant.numeric"
			settings: {
				foreground: "#46A609"
			}
		}
		{
			title: "Built-in constant"
			scope: "constant.language"
			settings: {
				font_style: ""
				foreground: "#39946A"
			}
		}
		{
			title: "User-defined constant"
			scope: "constant.character, constant.other"
			settings: {

			}
		}
		{
			title: "Variable"
			scope: "variable.language, variable.other"
			settings: {
				font_style: ""
			}
		}
		{
			title: "Keyword"
			scope: "keyword, support.constant.property-value, constant.other.color"
			settings: {
				font_style: ""
				foreground: "#927C5D"
			}
		}
		{
			title: "Keyword -> Unit"
