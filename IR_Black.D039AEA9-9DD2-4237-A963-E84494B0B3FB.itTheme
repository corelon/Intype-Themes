{
	title: "IR_Black"
	settings: [
		{
			settings: {
				background: "#000000"
				caret: "#FFFFFF"
				foreground: "#EDEDED"
				invisibles: "#CAE2FB3D"
				line_highlight: "#FFFFFF24"
				selection: "#333333"
			}
		}
		{
			title: "Comment"
			scope: "comment"
			settings: {
				font_style: ""
				foreground: "#7C7C7C"
			}
		}
		{
			title: "Entity"
			scope: "entity"
			settings: {
				font_style: ""
				foreground: "#FFD2A7"
			}
		}
		{
			title: "Keyword"
			scope: "keyword"
			settings: {
				font_style: ""
				foreground: "#96CBFE"
			}
		}
		{
			title: "Keyword.control"
			scope: "keyword.control"
			settings: {
				font_style: ""
				foreground: "#96CBFE"
			}
		}
		{
			title: "Keyword.Operator"
			scope: "keyword.operator"
			settings: {
				foreground: "#EDEDED"
			}
		}
		{
			title: "Class"
			scope: "entity.name.type"
			settings: {
				font_style: "underline"
				foreground: "#FFFFB6"
			}
		}
		{
			title: "Support"
			scope: "support"
			settings: {
				font_style: ""
				foreground: "#FFFFB6"
			}
		}
		{
			title: "Storage"
			scope: "storage"
			settings: {
				font_style: ""
				foreground: "#CFCB90"
			}
		}
		{
			title: "Storage.modifier"
			scope: "storage.modifier"
			settings: {
				font_style: ""
				foreground: "#96CBFE"
			}
		}
		{
			title: "Constant"
			scope: "constant"
			settings: {
				font_style: ""
				foreground: "#99CC99"
			}
		}
		{
			title: "String"
			scope: "string"
			settings: {
				font_style: "bold"
				foreground: "#A8FF60"
			}
		}
		{
			title: "Number"
			scope: "constant.numeric"
			settings: {
				font_style: "bold"
				foreground: "#FF73FD"
			}
		}
		{
			title: "Punctuation"
			scope: "punctuation"
			settings: {
