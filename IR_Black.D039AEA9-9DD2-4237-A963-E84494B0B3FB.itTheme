			title: "Variable"
			scope: "variable"
			settings: {
				font_style: ""
				foreground: "#C6C5FE"
			}
		}
		{
			title: "Invalid – Deprecated"
			scope: "invalid.deprecated"
			settings: {
				font_style: "italic underline"
				foreground: "#FD5FF1"
			}
		}
		{
			title: "Invalid – Illegal"
			scope: "invalid.illegal"
			settings: {
				background: "#562D56BF"
				foreground: "#FD5FF1"
			}
		}
		{
			title: "-----------------------------------"
			settings: {

			}
		}
